page{
    height: 100%;
}
.container{
    background-color: #f5f5f9;
    justify-content: initial;
}
.cms-box {
  display: block;
  width: 700rpx;
  height: 122rpx;
  line-height: 122rpx;
  padding: 0 66rpx;
  font-size: 50rpx;
  background: #fff;
}
.w-header {
  font-size: 40rpx;
  color: #fff;
}
.myStyle {
  border: 1px solid #fff;
  color: #fff;
}
.cms-box .left, .cms-box .right{
  display: inline-block;
  width: 350rpx;
}
.left {
  text-align: left;
}
.right {
  text-align: right;
}
.margin-20 {
  margin-bottom: 20rpx;
}
.margin-10 {
  margin-bottom: 10rpx;
}
.save-btn {
    width: 690rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    margin-top:30rpx; 
    border-radius: 6rpx;
    box-sizing: border-box;
    color:#fff;
}