page {
	min-height: 100%;
}
.container{
  min-height: 100%;
  background-color: #f4f4f4;
  font-size: 24rpx;
}

.userbox {
  overflow: hidden;
  display: flex;
	flex-direction: column;
  width: 100%;
  height: 300rpx;
  align-items: center;
  justify-content: center;
}
.userbox .userinfo {
	display: flex;
	flex-direction: column;
  width: 750rpx;
  z-index: 100;
  margin-top: 100rpx;
	align-items: center;
  justify-content: center;
}

.userbox .userinfo .userinfo-avatar {
  display: block;
	width: 160rpx;
	height: 160rpx;
  margin: 20rpx 0;
  border: 5rpx solid #eee;
  border-radius: 50%;
}

.userbox .userinfo .userinfo-avatar-no {
  display: block;
	width: 160rpx;
	height: 160rpx;
  margin: 20rpx 0;
  border: 5rpx solid #eee;
  border-radius: 5%;
}

.userbox .userinfo .userinfo-text{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.userbox .userinfo .userinfo-text .userinfo-nickname {
  font-size: 28rpx;
  font-weight: bold;
  color: #333;
}

.userbox .userInfoMask{
  width: 100%;
  height: 350rpx;
  z-index: 1;
  position: relative;
  bottom: 0rpx;
  border-top-left-radius: 70%;
  border-top-right-radius: 70%;
  background-color: #FFF;
}

.phoneNumber{
  align-items: center;
  font-size: 24rpx;
  color: #333333;
  margin-bottom: 20rpx;
}

.pin-phoneNumber{
  align-items: center;
  margin-bottom: 20rpx;
  padding: 0 10rpx;
  font-size: 20rpx;
  color: #ff5577;
  background-color: #ffe8ee;
  border-radius: 1% solid #ffe8ee;
}

.userbox .my-sign{
  position: absolute;
  top: 10rpx;
  right: 30rpx;
  z-index: 100;
  margin-top: 20rpx;
  font-size: 24rpx;
}

.h-contents{
  z-index: 100;
  width: 100%;
  height: 125rpx;
  /* margin-bottom: 20rpx; */
  background-color: #FFF;
  display: flex;
  flex-direction: row;

}

.h-contents .a-item{
  width: 27%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  color: #333;
}


.h-contents .a-item .my{
  display: flex;
  flex-direction: row;
}

.h-contents .a-item-no-right-border{
  width: 19%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  color: #333;
}

.user-menu{
  width: 100%;
  height: auto;
  background: #FFF;
  border-bottom: 1px solid #EFEFEF;
}

.user-menu .item{
  float: left;
  width: 249rpx;
  height: 167.5rpx;/*187.5*/
  border-top: 1px solid #EFEFEF; 
  border-right: 1px solid #EFEFEF;
  text-align: center;
}

.user-menu .item .a{
  display: flex;
  width: 100%;
  height: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

#contact-btn {
  background:none;
  padding: 0;
  margin: 0;
  font-size: 100%;
  line-height: normal;
  opacity: 1;
  display: inline;
  border-radius:0rpx
}
#contact-btn::after{ border: none; border-radius:0rpx }

.user-menu .item.no-border-right {
  border-right: 0;
}
.user-menu .item.item-bottom {
  border-bottom: 1px solid #EFEFEF;
}
.user-menu .item.no-border-bottom{
  border-bottom: 1px solid #EFEFEF;
  border-right: none;
}
.user-menu .icon{
  margin: 0 auto;
  display: block;
  height: 60rpx;
  width: 60rpx;
  margin-bottom: 16rpx;
}

.user-menu .txt{
  display: block;
  height: auto;
  width: 100%;
  color:#333;
}
.user-menu .price-box {
  width: 100%;
  margin: 10rpx 0;
}
.user-menu .price-box .count {
  color: #FF5777;
  font-weight: bold;
}

.my-item{
    width: 30%;
    background: url(https://cdn.it120.cc/images/weappshop/arrow-right.png) no-repeat 750rpx center;
    background-size: 16rpx auto,750rpx auto;
    margin: 20rpx 0; 
    border-bottom: 1px solid #f4f4f4;
    height: 80rpx;
    line-height: 100rpx;
    padding-left: 100rpx;
}

.version {
  width:100%;
  height: 30rpx;
	line-height: 30rpx;
	font-size: 24rpx;
	text-align: center;  
}
.sale-info {
  width: 100%;
  height: 125rpx;
  padding: 20rpx 0;
  margin-bottom: 20rpx;
  background-color: #FFF;
  display: flex;
  flex-direction: row;
}
.sale-info .s-box {
  width: 50%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border-right: 1px solid #EFEFEF;
}
.sale-info .s-box .num {
  padding:0 10rpx;
  font-weight: bold;
  margin-top: 4rpx;
  font-size: 24rpx;
}
.icon-style {
  margin: 7px 0 5px 0;
}

